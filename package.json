{
  "name": "fryerpan",
  "version": "1.0.0",
  "main": "index.js",
  "author": "daniel lopez",
  "license": "MIT",
  "scripts": {
    "start": "parcel index.html --no-cache",
    "start-bs": "yarn start & yarn bs",
    "bs": "browser-sync start --proxy \"localhost:1234\"",
