{
  "name": "fullstack-frypan",
  "version": "1.0.0",
  "main": "index.js",
  "author": "daniel lopez",
  "license": "MIT",
  "scripts": {
    "start": "parcel index.html --no-cache",
    "start:app": "PORT=1234 nodemon --watch components _dev/dev-app.js",
    "start:server": "PORT=5678 nodemon --watch api _dev/index.js",
    "dev": "npm run start:app & npm run start:server",
    "build": "parcel build index.html -d build --public-url ./",
    "deploy": "now && now alias",
    "now-build": "npm run build",
    "start-bs": "npm run start:app & npm run bs",
    "bs": "browser-sync start --proxy \"localhost:1234\"",
    "predeploy:surge": "npm run build",
    "deploy:surge": "cd build && surge --domain fryerpan.surge.sh"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "styled-components": "^4.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "browser-sync": "^2.26.3",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-html": "^4.0.6",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.4",
    "http-proxy-middleware": "^0.19.1",
    "nodemon": "^1.18.10",
    "parcel-bundler": "^1.11.0",
    "prettier": "^1.14.3"
  },
  "proxy": "http://localhost:5678"
}